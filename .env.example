# Server Configuration
PORT=3001

# EOD Historical Data API
# Get your API key from: https://eodhd.com
EOD_API_KEY=your_eod_api_key_here

# Optional: Rate limiting (milliseconds between API calls)
BACKTEST_RATE_LIMIT_MS=100

# Optional: Maximum concurrent API requests
BACKTEST_MAX_CONCURRENT_REQUESTS=5

# Optional: Cache configuration
CACHE_ENABLED=true
CACHE_DIRECTORY=./cache
CACHE_MAX_AGE_DAYS=30

# Optional: Logging level (debug, info, warn, error)
LOG_LEVEL=info

# Optional: Output directory for backtest results
OUTPUT_DIRECTORY=./output